PKGNAME=coreutils
PKGSEC=utils
PKGDEP="glibc linux-pam acl libcap openssl gmp"
PKGDES="Basic and important programs for a basic GNU userland"

AUTOTOOLS_AFTER="--with-openssl \
                 --enable-no-install-program=hostname,kill,uptime \
                 --enable-install-program=arch"
RPMEXTRAPROVIDE="/bin/env /sbin/env"
